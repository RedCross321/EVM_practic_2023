void setup() {
  pinMode(1, OUTPUT);
}

void loop() {
  digitalWrite(1, HIGH);
  digitalWrite(1, LOW);
  delay(5000);
}
---------------------------
void setup() {
  pinMode(1, OUTPUT);
  pinMode(13, INPUT_PULLUP);
}

void loop() {
  if (digitalRead(13) == HIGH) {
    digitalWrite(1, HIGH);
  }
  else {
    digitalWrite(1, LOW);
  }
}
---------------------------
